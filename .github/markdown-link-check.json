{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^http://127.0.0.1"
    },
    {
      "pattern": "^https://crates.io/crates/"
    },
    {
      "pattern": "^https://developer.android.com"
    },
    {
      "pattern": "^https://source.android.com"
    },
    {
      "pattern": "^https://keith.github.io/xcode-man-pages"
    },
    {
      "pattern": "^https://appium.io"
    },
    {
      "pattern": "^https://codecov.io"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "Accept": "text/html",
        "User-Agent": "Mozilla/5.0"
      }
    }
  ],
  "timeout": "30s",
  "retryOn429": true,
  "retryCount": 5,
  "fallbackRetryDelay": "60s",
  "aliveStatusCodes": [200, 206, 301, 302, 307, 308, 403, 429, 999]
}
